{
	"presets": [
		"@babel/preset-react",
		[
			"@babel/preset-typescript",
			{
				"isTSX": true,
				"allExtensions": true
			}
		],
		[
			"@babel/preset-env",
			{
				"useBuiltIns": "usage",
				"corejs": {
					"version": 3
				}
			}
		]
	],
	"plugins": [
		"add-react-displayname",
		"babel-plugin-typescript-to-proptypes"
