{
  "name": "jhwd-electron-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "rimraf build && webpack --config webpack.prod.js",
    "pack": "rimraf dist && electron-builder --dir",
    "dist": "rimraf dist && npm run build && electron-builder -w -m",
    "deploy": "rimraf dist && npm run build && electron-builder build -w -m --publish always",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ."
  },
  "author": "Jonathan Hamilton",
  "license": "MIT",
  "dependencies": {
    "electron-updater": "^4.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.3",
    "postcss": "^7.0.29",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": "electron 6.0.1"
}
