{
  "name": "jhwd-electron-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "dev": "webpack serve --config webpack.dev.js",
    "build": "rimraf build && webpack --config webpack.prod.js",
    "pack": "rimraf dist && electron-builder --dir",
    "dist:mac": "rimraf dist && npm run build && electron-builder -m",
    "dist:win": "rimraf dist && npm run build && electron-builder -w",
    "deploy:mac": "rimraf dist && npm run build && electron-builder build -m --publish always",
    "deploy:win": "rimraf dist && npm run build && electron-builder build -w --publish always",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ."
  },
  "author": "Jonathan Hamilton",
  "license": "MIT",
  "dependencies": {
    "electron-updater": "^4.3.5",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.12.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "core-js": "^3.6.5",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.21.4",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.0.0",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "webpack": "^5.1.2",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0",
    "webpack-node-externals": "^2.5.2"
  },
  "browserslist": "last 2 electron versions"
}
