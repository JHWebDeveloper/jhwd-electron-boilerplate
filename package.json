{
  "name": "jhwd-electron-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "rimraf build && webpack --config webpack.prod.js",
    "pack": "rimraf dist && electron-builder --dir",
    "dist:mac": "rimraf dist && npm run build && electron-builder -m",
    "dist:win": "rimraf dist && npm run build && electron-builder -w",
    "deploy:mac": "rimraf dist && npm run build && electron-builder build -m --publish always",
    "deploy:win": "rimraf dist && npm run build && electron-builder build -w --publish always",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ."
  },
  "author": "Jonathan Hamilton",
  "license": "MIT",
  "dependencies": {
    "electron-updater": "^4.3.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^6.1.0",
    "core-js": "^3.6.5",
    "css-loader": "^4.2.2",
    "cssnano": "^4.1.10",
    "electron": "^10.1.1",
    "electron-builder": "^22.8.0",
    "eslint": "^7.8.1",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "mini-css-extract-plugin": "^0.11.0",
    "nodemon": "^2.0.4",
    "postcss": "^7.0.32",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  },
  "browserslist": "chrome 85"
}
