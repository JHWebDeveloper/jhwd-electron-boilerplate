{
  "name": "jhwd-electron-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "rimraf build && webpack --config webpack.prod.js",
    "pack": "rimraf dist && electron-builder --dir",
    "dist": "rimraf dist && npm run build && electron-builder -w -m",
    "deploy": "rimraf dist && npm run build && electron-builder build -w -m --publish always",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ."
  },
  "author": "Jonathan Hamilton",
  "license": "MIT",
  "dependencies": {
    "electron-updater": "^4.2.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/register": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^5.0.5",
    "core-js": "^3.4.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "electron": "^7.1.1",
    "electron-builder": "^22.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nodemon": "^2.0.2",
    "postcss": "^7.0.21",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": "electron 6.0.1"
}
